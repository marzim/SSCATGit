<?xml version="1.0" encoding="utf-8"?>
<myTypes:BaseUserControl
	x:Class="Sscat.Wpf.Gui.ClientPane" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:myTypes="clr-namespace:Sscat.Wpf.Gui">
	<StackPanel>
		<StackPanel
			Orientation="Horizontal">
			<Button
				Padding="10"
				Click="ButtonGenerateScriptsClick">Generate Scripts</Button>
			<Button
				Padding="10"
				Click="ButtonPlayScriptsClick">Run Scripts</Button>
			<Button
				Padding="10"
				Click="ButtonUpdateConfigurationClick">Settings</Button>
		</StackPanel>
		<DockPanel
			Name="panelContent"></DockPanel>
	</StackPanel>
</myTypes:BaseUserControl>